{
  "name": "@thingsboard/n8n-nodes-thingsboard",
  "version": "1.1.0",
  "description": "n8n node for ThingsBoard",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "thingsboard"
  ],
  "license": "MIT",
  "homepage": "https://thingsboard.io/docs/samples/analytics/n8n-node/",
  "author": {
    "name": "Andrii Landiak",
    "email": "alandiak@thingsboard.io"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thingsboard/thingsboard-n8n-node.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "dist/nodes/thingsboard/ThingsBoard.node.js",
  "scripts": {
    "build": "npx rimraf dist && npx tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": ["dist", "images", "README.md", "LICENSE.md", "package.json"],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ThingsBoardApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/thingsboard/ThingsBoard.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.19.19",
    "@types/request": "^2.48.13",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.1",
    "n8n-core": "^1.114.0",
    "n8n-workflow": "^1.112.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {},
  "overrides": {
    "@azure/identity": "^4.2.1",
    "form-data": "^4.0.4",
    "semver": "^5.7.2",
    "@n8n/imap": {
      "semver": "^5.7.2",
      "utf7": {
        "semver": "^5.7.2"
      }
    },
    "string-width-cjs": "npm:string-width@4.2.3",
    "strip-ansi-cjs": "npm:strip-ansi@6.0.1",
    "wrap-ansi-cjs": "npm:wrap-ansi@7.0.0"
  },
  "bugs": {
    "url": "https://github.com/thingsboard/thingsboard-n8n-node/issues"
  }
}
